<% layout('layouts/boilerplate') %>
<div class="col-6 offset-3">
    <form id="upload_form" action="/client" enctype="multipart/form-data" class="validated-form mt-5" method="POST">
        <div class="mb-3">
            <label class="form-label" for="file"><strong>Select CSV File:</strong></label><br>
            <input type="file" name="file" id="file">
        </div>
        <button class="btn btn-primary">Submit</button>
    </form>
</div>